<?xml version="1.0" encoding="utf-8"?>
<Database>
  <Victories>
    <Update>
      <Where VictoryType="VICTORY_MODERN_CULTURE"/>
      <Set EnabledByDefault='0'/>
    </Update>
  </Victories>

  <Strategies>
    <Update>
      <Where LegacyPathType="LEGACY_PATH_MODERN_CULTURE"/>
      <Set MinNumConditionsNeeded='100'/>
    </Update>

    <Update>
      <Where LegacyPathType="LEGACY_PATH_MODERN_CULTURE"/>
      <Set MaxNumConditionsNeeded='100'/>
    </Update>

    <Update>
      <Where LegacyPathType='LEGACY_PATH_MODERN_CULTURE'/>
      <Set MinConditionPercentage='100'/>
    </Update>
  </Strategies>

  <Wonders>
    <Update>
      <Where ConstructibleType='WONDER_WORLDS_FAIR'/>
      <Set MaxWorldInstances='0'/>
    </Update>
    <Update>
      <Where ConstructibleType='WONDER_WORLDS_FAIR'/>
      <Set MaxPerPlayer='0'/>
    </Update>
  </Wonders>

  <AgeProgressionMilestones>
    <Update>
      <Where LegacyPathType='LEGACY_PATH_MODERN_CULTURE'/>
      <Set AgeProgressionAmount='0'/>
    </Update>
	</AgeProgressionMilestones>
</Database>