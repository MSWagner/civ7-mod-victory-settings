<?xml version="1.0" encoding="utf-8"?>
<Database>
  <Victories>
    <Update>
      <Where VictoryType="VICTORY_MODERN_ECONOMIC"/>
      <Set EnabledByDefault='0'/>
    </Update>
  </Victories>

  <Strategies>
    <Update>
      <Where LegacyPathType="LEGACY_PATH_MODERN_ECONOMIC"/>
      <Set MinNumConditionsNeeded='100'/>
    </Update>

    <Update>
      <Where LegacyPathType="LEGACY_PATH_MODERN_ECONOMIC"/>
      <Set MaxNumConditionsNeeded='100'/>
    </Update>

    <Update>
      <Where LegacyPathType='LEGACY_PATH_MODERN_ECONOMIC'/>
      <Set MinConditionPercentage='100'/>
    </Update>
  </Strategies>

  <GreatPersonIndividuals>
    <Update>
      <Where GreatPersonIndividualType='GREAT_PERSON_INDIVIDUAL_VICTORY_JOHN_MAYNARD_KEYNES'/>
      <Set ActionCharges='0'/>
    </Update>
  </GreatPersonIndividuals>

  <AgeProgressionMilestones>
    <Update>
      <Where LegacyPathType='LEGACY_PATH_MODERN_ECONOMIC'/>
      <Set AgeProgressionAmount='0'/>
    </Update>
	</AgeProgressionMilestones>
</Database>