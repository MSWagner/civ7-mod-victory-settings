<?xml version="1.0" encoding="utf-8"?>
<Database>
  <Victories>
    <Update>
      <Where VictoryType="VICTORY_MODERN_SCIENCE"/>
      <Set EnabledByDefault='0'/>
    </Update>
    </Victories>

  <Strategies>
    <Update>
      <Where LegacyPathType="LEGACY_PATH_MODERN_SCIENCE"/>
      <Set MinNumConditionsNeeded='100'/>
    </Update>

    <Update>
      <Where LegacyPathType="LEGACY_PATH_MODERN_SCIENCE"/>
      <Set MaxNumConditionsNeeded='100'/>
    </Update>

    <Update>
      <Where LegacyPathType='LEGACY_PATH_MODERN_SCIENCE'/>
      <Set MinConditionPercentage='100'/>
    </Update>
  </Strategies>

  <Projects>
    <Update>
      <Where ProjectType='PROJECT_LAUNCH_ROCKET'/>
      <Set MaxPlayerInstances='0'/>
    </Update>
  </Projects>

  <AgeProgressionMilestones>
    <Update>
      <Where LegacyPathType='LEGACY_PATH_MODERN_SCIENCE'/>
      <Set AgeProgressionAmount='0'/>
    </Update>
	</AgeProgressionMilestones>
</Database>